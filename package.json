{
  "name": "apontamento",
  "version": "1.0.0",
  "main": "electron.js",
  "scripts": {
    "ng": "ng",
    "dev": "concurrently \"ng serve\" \"wait-on http://localhost:4200 && electron . --serve\"",
    "build": "ng build",
    "electron": "ng build --base-href ./ && electron .",
    "electron:build": "ng build -c production && electron-builder --win portable"
  },
  "author": {
    "name": "henrique.formiga",
    "email": "henriqueguedesformiga@gmail.com"
  },
  "private": true,
  "build": {
    "extends": null,
    "appId": "com.seuapp.apontamento",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/browser/**/*",
      "electron.js",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@angular/cdk": "^19.2.17",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.17",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "papaparse": "^5.5.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.11",
    "@angular/cli": "^19.2.11",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "@types/papaparse": "^5.3.16",
    "concurrently": "^9.1.2",
    "electron": "^36.3.1",
    "electron-builder": "^26.0.12",
    "electron-debug": "^4.1.0",
    "electron-reloader": "^1.2.3",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2",
    "wait-on": "^8.0.3"
  }
}
